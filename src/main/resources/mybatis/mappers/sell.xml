<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
						"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
						
<mapper namespace="com.kosta.clothes.dao.SellDAO">
	<select id="getNextIndividualNo" resultType="Integer">
		<![CDATA[
			select individual_seq.nextval from dual
		]]>
	</select>
	
	<insert id="insertIndividual" parameterType="Sell">
		<![CDATA[
			insert into individual(ino,iname,ititle,icontent,istatus,ifileids,idealType,
			addressCity,addressTown,userno,applycount,price,regDate)
			values (#{ino},#{iname},#{ititle},#{icontent},#{istatus},#{ifileids,jdbcType=VARCHAR},
			#{idealType},#{addressCity},#{addressTown},#{userno},#{applycount},#{price},sysdate)
	
		]]>
	</insert>
</mapper>	