<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
						"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
						
<mapper namespace="com.kosta.clothes.dao.SharingDAO">
	<insert id="insertSharing" parameterType="sharing">
		<![CDATA[
			insert into sharing(sid,stitle,scontent,simage1,simage2,simage3,sdealType,
			addressCity,addressTown,regDate)
			values (sharing_seq.nextval,#{stitle},#{scontent},#{simage1,jdbcType=VARCHAR},#{simage2,jdbcType=VARCHAR},#{simage3,jdbcType=VARCHAR},
			#{sdealType},#{addressCity},#{addressTown},sysdate)
	
		]]>
	</insert>
	
	<select id="selectSharingList" resultType="com.kosta.clothes.bean.Sharing">
		<![CDATA[
			select * from sharing order by sid desc
		]]>
	</select>
</mapper>

















